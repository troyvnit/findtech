@model FindTech.Web.Models.ArticleViewModel
@{
    var article = Model;
}
<a href="@Url.Action("Detail", "Article", new { seoTitle = article.SeoTitle })" class="banner">
    <div class="box">
        <img src="@article.Avatar" width="253" height="253" alt="">
        <div class="overlay">
            <h2 class="title">@article.Title</h2>
            <div class="description">@article.Description</div>
        </div>
    </div>
</a>
