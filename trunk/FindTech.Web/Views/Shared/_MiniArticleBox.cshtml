@model FindTech.Web.Models.ArticleViewModel
@{
    var article = Model;
}
<div class="make-wrapper">
    <a href="@Url.Action("Detail", "Article", new { seoTitle = article.SeoTitle })">
        <img src="@article.Avatar" width="150" height="100" alt="">
        <a href="@Url.Action("Detail", "Article", new { seoTitle = article.SeoTitle })"><h2 class="title">@article.Title</h2></a>
    </a>
</div>
